  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "build:apps:gateway": "npm run build gateway",
    "build:apps:authentication": "npm run build authentication",
    "build:lib:cache": "npm run build cache",
    "build:lib:common": "npm run build common",
    "build:lib:database": "npm run build database",
    "build:all": "npm run build:lib:cache; npm run build:lib:common; npm run build:lib:database; npm run build:apps:gateway; npm run build:apps:authentication",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --detectOpenHandles",
    "test:watch": "jest --detectOpenHandles --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/gateway/test/jest-e2e.json; jest --config ./apps/authentication/test/jest-e2e.json",
    "test:all": "npm run test; npm run test:e2e",
    "typeorm:db": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config libs/database/src/ormconfig.ts",
    "typeorm:db:show": "npm run typeorm:db migration:show",
    "typeorm:db:run": "npm run typeorm:db migration:run",
    "typeorm:db:revert": "npm run typeorm:db migration:revert",
    "ncu": "ncu -u"
  },
