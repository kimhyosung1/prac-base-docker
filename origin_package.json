  "jest": {
    "collectCoverage": true,
    "coverageReporters": [
      "json",
      "html"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "@libs/aws/(.*)": "<rootDir>/libs/aws/src/$1",
      "@libs/aws": "<rootDir>/libs/aws/src",
      "@libs/common/(.*)": "<rootDir>/libs/common/src/$1",
      "@libs/common": "<rootDir>/libs/common/src",
      "@libs/cache/(.*)": "<rootDir>/libs/cache/src/$1",
      "@libs/cache": "<rootDir>/libs/cache/src",
      "@libs/database/(.*)": "<rootDir>/libs/database/src/$1",
      "@libs/database": "<rootDir>/libs/database/src",
      "@libs/model/(.*)": "<rootDir>/libs/model/src/$1",
      "@libs/model": "<rootDir>/libs/model/src"
    }
  }
